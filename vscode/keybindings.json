// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "shift+cmd+f",
        "command": "filesExplorer.findInFolder"
    },
    {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "cmd+shift+[",
        "command": "workbench.action.navigateToLastEditLocation"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "cmd+alt+t",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.editorLayoutSingle"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.editorLayoutTwoColumns"
    },
    {
        "key": "alt+cmd+z",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "alt+a",
        "command": "explorer.newFile"
    },
    {
        "key": "shift+alt+a",
        "command": "explorer.newFolder"
    },
    {
        "key": "alt+3",
        "command": "workbench.action.editorLayoutThreeColumns"
    },
    {
        "key": "alt+4",
        "command": "workbench.action.editorLayoutTwoByTwoGrid"
    },
    {
        "key": "alt+5",
        "command": "workbench.action.editorLayoutTwoRowsRight"
    },
    {
        "key": "alt+6",
        "command": "workbench.action.editorLayoutTwoColumnsBottom"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.tasks.reRunTask",
        "when": "!terminalFocus"
    },
    {
        "key": "alt+r",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "shift+alt+enter",
        "command": "workbench.action.toggleEditorWidths"
    },
    {
        "key": "home",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {"text": "\u0001"},
    },
    {
        "key": "end",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {"text": "\u0005"},
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+1",
        "command": "workbench.view.explorer",
    },
    {
        "key": "cmd+9",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "alt+k",
        "command": "cursorUp",
        "when": "textInputFocus"
    },
    {
        "key": "alt+j",
        "command": "cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "alt+h",
        "command": "-gitlens.showQuickFileHistory",
        "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
    },
    {
        "key": "alt+h",
        "command": "cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "alt+l",
        "command": "cursorRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+h",
        "command": "cursorWordLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "cursorWordEndRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+cmd+up",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalProcessSupported"
    },
    {
        "key": "alt+cmd+h",
        "command": "references-view.showCallHierarchy",
        "when": "editorHasCallHierarchyProvider"
    },
    {
        "key": "shift+cmd+o",
        "command": "workbench.action.quickOpen",
        "when": "editorTextFocus"
    },{
        "key": "cmd+f12",
        "command": "workbench.action.gotoSymbol",
        "when": "editorTextFocus"
    },{
        "key": "shift+cmd+u",
        "command": "workbench.panel.output.focus",
        "when": "!workbench.panel.output.active"
    },{
        "key": "alt+f12",
        "command": "terminal.focus",
        "when": "!terminalEditorFocus"
    },{
        "key": "cmd+[IntlBackslash]",
        "command": "workbench.action.focusActiveEditorGroup"
    },
    {
        "key": "alt+k",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "alt+j",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "cmd+d",
        "command": "editor.action.duplicateSelection"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+backspace",
        "command": "editor.action.deleteLines",
        "when": "editorTextFocus && !editorReadonly"
    },{
        "key": "ctrl+j",
        "command": "editor.action.showHover",
        "when": "editorTextFocus"
    },{
        "key": "cmd+e",
        "command": "workbench.action.openRecent",
    }
]
