// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "shift+cmd+f",
        "command": "filesExplorer.findInFolder"
    },
    {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "cmd+]",
        "command": "-editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+alt+t",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "shift+cmd+]",
        "command": "-workbench.action.nextEditor"
    },
    {
        "key": "ctrl+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "shift+cmd+[",
        "command": "-workbench.action.previousEditor"
    },
    {
        "key": "alt+cmd+m",
        "command": "editor.action.toggleMinimap"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.editorLayoutSingle"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.editorLayoutTwoColumns"
    },
    {
        "key": "alt+cmd+z",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "cmd+k",
        "command": "-git.commitAll",
        "when": "!inDebugMode && !terminalFocus"
    },
    {
        "key": "cmd+\\",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "alt+a",
        "command": "explorer.newFile"
    },
    {
        "key": "shift+alt+a",
        "command": "explorer.newFolder"
    },
    {
        "key": "shift+alt+a",
        "command": "-editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+3",
        "command": "workbench.action.editorLayoutThreeColumns"
    },
    {
        "key": "alt+4",
        "command": "workbench.action.editorLayoutTwoByTwoGrid"
    },
    {
        "key": "alt+5",
        "command": "workbench.action.editorLayoutTwoRowsRight"
    },
    {
        "key": "alt+6",
        "command": "workbench.action.editorLayoutTwoColumnsBottom"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "-notebook.cell.focusInOutput",
        "when": "notebookCellHasOutputs && notebookEditorFocused"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "-notebook.focusNextEditor",
        "when": "notebookEditorFocused && notebookOutputFocused"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.tasks.reRunTask",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.tasks.reRunTask"
    },
    {
        "key": "alt+r",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "shift+alt+enter",
        "command": "workbench.action.toggleEditorWidths"
    },
]
