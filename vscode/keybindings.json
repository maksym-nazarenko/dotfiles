// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "shift+cmd+f",
        "command": "filesExplorer.findInFolder"
    },
    {
        "key": "cmd+l",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "cmd+[",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "cmd+]",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "cmd+alt+t",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+right",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+left",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "alt+cmd+m",
        "command": "editor.action.toggleMinimap"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.editorLayoutSingle"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.editorLayoutTwoColumns"
    },
    {
        "key": "alt+cmd+z",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "alt+a",
        "command": "explorer.newFile"
    },
    {
        "key": "shift+alt+a",
        "command": "explorer.newFolder"
    },
    {
        "key": "alt+3",
        "command": "workbench.action.editorLayoutThreeColumns"
    },
    {
        "key": "alt+4",
        "command": "workbench.action.editorLayoutTwoByTwoGrid"
    },
    {
        "key": "alt+5",
        "command": "workbench.action.editorLayoutTwoRowsRight"
    },
    {
        "key": "alt+6",
        "command": "workbench.action.editorLayoutTwoColumnsBottom"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.tasks.reRunTask",
        "when": "!terminalFocus"
    },
    {
        "key": "alt+r",
        "command": "workbench.files.action.showActiveFileInExplorer"
    },
    {
        "key": "shift+alt+enter",
        "command": "workbench.action.toggleEditorWidths"
    },
    {
        "key": "cmd+k",
        "command": "-git.commitAll",
        "when": "!inDebugMode && !terminalFocus"
    },
    {
        "key": "home",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {"text": "\u0001"},
    },
    {
        "key": "end",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {"text": "\u0005"},
    },
    {
        "key": "cmd+2",
        "command": "-workbench.action.focusSecondEditorGroup",
    },
    {
        "key": "cmd+9",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+9",
        "command": "-workbench.action.lastEditorInGroup"
    },
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+numpad9",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "shift+escape",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus && !inDebugRepl && !problemFocus && !terminalFocus"
    },
    {
        "key": "cmd+1",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+numpad1",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+3",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "searchViewletVisible"
    },
    {
        "key": "cmd+numpad3",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "searchViewletVisible"
    },
    {
        "key": "cmd+5",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+numpad5",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+1",
        "command": "-workbench.action.focusFirstEditorGroup",
        "when": "editorFocus"
    },
    {
        "key": "cmd+1",
        "command": "workbench.view.explorer",
    },
    {
        "key": "cmd+9",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "cmd+9",
        "command": "-workbench.view.scm",
        "when": "editorFocus && workbench.scm.active"
    },
    {
        "key": "cmd+1",
        "command": "-workbench.action.focusFirstEditorGroup"
    },
    {
        "key": "cmd+1",
        "command": "-workbench.view.explorer",
        "when": "editorFocus && viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.tasks.reRunTask"
    },
    {
        "key": "alt+k",
        "command": "cursorUp",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+p",
        "command": "-cursorUp",
        "when": "textInputFocus"
    },
    {
        "key": "alt+j",
        "command": "cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+n",
        "command": "-cursorDown",
        "when": "textInputFocus"
    },
    {
        "key": "alt+h",
        "command": "-gitlens.showQuickFileHistory",
        "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
    },
    {
        "key": "alt+h",
        "command": "cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+b",
        "command": "-cursorLeft",
        "when": "textInputFocus"
    },
    {
        "key": "alt+l",
        "command": "cursorRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+f",
        "command": "-cursorRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+j",
        "command": "-notebook.cell.joinBelow",
        "when": "notebookEditorFocused"
    },
    {
        "key": "ctrl+alt+h",
        "command": "cursorWordLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "cursorWordEndRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+cmd+up",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+up",
        "command": "-workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalProcessSupported"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "-workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && terminalProcessSupported"
    },
]
